<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="300.0" prefWidth="500.0" styleClass="ventanasModales" stylesheets="@MainCSS.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.GestionarCorredoresController">
   <children>
      <Label layoutX="34.0" layoutY="14.0" prefHeight="50.0" prefWidth="286.0" text="Eliminar corredor" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="btnGuardarNuevoCorredor" layoutX="368.0" layoutY="244.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="99.0" styleClass="guardarModal" stylesheets="@MainCSS.css" text="Confirmar" textFill="WHITE">
         <font>
            <Font name="SansSerif Regular" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnVolverNuevoCorredor" layoutX="250.0" layoutY="244.0" mnemonicParsing="false" onAction="#botonCerrarModal" prefHeight="41.0" prefWidth="99.0" styleClass="eliminarModal" stylesheets="@MainCSS.css" text="Cancelar" textFill="WHITE">
         <font>
            <Font name="SansSerif Regular" size="16.0" />
         </font>
      </Button>
      <PasswordField layoutX="67.0" layoutY="159.0" prefHeight="41.0" prefWidth="357.0" promptText="Ingrese su contraseña">
         <font>
            <Font size="17.0" />
         </font>
      </PasswordField>
      <Label layoutX="34.0" layoutY="64.0" prefHeight="50.0" prefWidth="296.0" text="Esta a punto de eliminar al corredor" textFill="WHITE">
         <font>
            <Font name="SansSerif Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="330.0" layoutY="64.0" prefHeight="50.0" prefWidth="157.0" text="Ejemplo" textFill="RED">
         <font>
            <Font name="SansSerif Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="100.0" prefHeight="50.0" prefWidth="463.0" text="para continuar ingrese su contraseña y confirme." textFill="WHITE">
         <font>
            <Font name="SansSerif Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="67.0" layoutY="201.0" prefHeight="26.0" prefWidth="183.0" text="Contraseña incorrecta" textFill="RED">
         <font>
            <Font name="SansSerif Bold" size="16.0" />
         </font>
      </Label>
   </children>
</Pane>
